{
  "name": "gems-admin",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve -c=dev --port=4200",
    "start:qa": "ng serve -c=qa --port=4200",
    "build": "ng build -c production --aot --build-optimizer --base-href /",
