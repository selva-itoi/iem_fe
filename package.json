    "@angular/platform-browser-dynamic": "^16.2.8",
    "@angular/router": "^16.2.8",
    "@asymmetrik/ngx-leaflet": "^16.0.1",
    "@ckeditor/ckeditor5-angular": "^7.0.0",
    "@ckeditor/ckeditor5-build-classic": "^39.0.1",
    "basic-ftp": "^5.0.3",
    "leaflet": "^1.9.4",
    "ngx-bootstrap": "^10.3.0",
    "primeicons": "^6.0.1",
